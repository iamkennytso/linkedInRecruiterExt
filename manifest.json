{
  "name": "LinkedIn Recruiter Helper",
  "description": "Hides certain information so their experience comes up higher on the page",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "LiR.png"
  },
  "content_scripts": [
    {
      "js": ["scripts.js"],
      "matches": [
        "https://www.linkedin.com/in/*"
      ]
    },
    {
      "js": ["searchScripts.js"],
      "matches": [
        "https://www.linkedin.com/search/results/people/*"
      ]
    }
  ]
}